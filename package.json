{
	"name": "sveltuse",
	"version": "0.0.3",
	"description": "Collection of Essential Svelte Composition Utilities",
	"main": "dist/index.js",
	"author": {
		"name": "RK Anik",
		"email": "rkanik.me@gmail.com",
		"url": "https://rkanik.pages.dev/"
	},
	"bugs": "https://github.com/rkanik/sveltuse/issues",
	"homepage": "https://github.com/rkanik/sveltuse",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"test": "playwright test",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"write-package-json": "node ./scripts/package-json-writer.js",
		"add-exports": "node ./scripts/add-exports-to-package-json.js",
		"package:publish": "standard-version && git push --follow-tags origin main && npm run package && npm publish",
		"gen:props": "createprops"
	},
	"peerDependencies": {
		"@popperjs/core": "^2.11.6",
		"classnames": "^2.3.2",
		"flowbite": "^1.6.3",
		"svelte": "^3.55.1"
	},
	"devDependencies": {
		"@iconify/svelte": "^3.1.3",
		"@playwright/test": "^1.33.0",
		"@sveltejs/adapter-cloudflare": "^2.2.4",
		"@sveltejs/adapter-vercel": "^2.4.3",
		"@sveltejs/kit": "^1.16.2",
		"@sveltejs/package": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"autoprefixer": "^10.4.14",
		"createprops": "^0.6.1",
		"esbuild": "0.17.18",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"mdsvex": "^0.10.6",
		"mdsvexamples": "^0.3.3",
		"postcss": "^8.4.23",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"prism-themes": "^1.9.0",
		"publint": "^0.1.9",
		"sass": "^1.62.1",
		"svelte": "^3.59.0",
		"svelte-check": "^3.3.1",
		"svelte-meta-tags": "^2.7.0",
		"svelte-preprocess": "^5.0.3",
		"svelte2tsx": "^0.6.13",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.5",
		"vitest": "^0.31.0"
	},
	"type": "module",
	"keywords": [
		"svelte",
		"sveltekit",
		"svelteuse",
		"sveltuse",
		"svelte-use",
		"utilities",
		"reusable",
		"composable",
		"composition",
		"functions"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/rkanik/sveltuse"
	},
	"dependencies": {
		"@docsearch/js": "^3.3.4",
		"@popperjs/core": "^2.11.7",
		"classnames": "^2.3.2",
		"flowbite": "^1.6.5",
		"flowbite-svelte": "^0.36.2",
		"pdfjs-dist": "^3.6.172"
	},
	"engines": {
		"npm": ">=7.0.0",
		"node": ">=16.0.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"contributors": [
		"RK Anik <rkanik.me@gmail.com>"
	]
}
